config wifi-device  radio0
	option type		mac80211
	option channel		11
	option hwmode		11g
	option distance		0
	option country		UA
	option path		'pci0000:00/0000:00:00.0'
	option htmode		HT40

config wifi-iface
	option device		radio0
	option network		lan
	option mode		ap
	option ssid		'Own Little Wi-Fi'
	option encryption	psk2
	option key		'OWN_WIFI_PASS_PLACEHOLDER'
