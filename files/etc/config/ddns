config ddns "global"
	option date_format "%F %R"
	option log_lines "250"
	option allow_local_ip "0"
config service "myddns_ipv4"
	option enabled		"1"
	option update_url	"https://[USERNAME]:[PASSWORD]@updates.dnsomatic.com/nic/update?hostname=all.dnsomatic.com&myip=[IP]"
	option use_https	"1"
	option cacert		"/etc/ssl/certs/ca-certificates.crt"
	option lookup_host	"DDNS_HOST_PLACEHOLDER"
	option domain		"dnsomatic.com"
	option username		"DNSOMATIC_USER_PLACEHOLDER"
	option password		"DNSOMATIC_PASS_PLACEHOLDER"
	option interface	"wan"
	option ip_source	"web"
	option ip_url		"http://myip.dnsomatic.com/"
